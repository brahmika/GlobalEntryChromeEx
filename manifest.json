{
   "manifest_version": 3,
   "name": "Global Entry Drop",
   "version": "1.0",
   "description": "Find global entry interviews for your convinent dates.",
   "action": {
    "default_popup": "popup/popup.html"
   },
   "icons": {
        "16":"images/internet16x16.png",
        "24":"images/internet24x24.png",
        "32":"images/internet32x32.png",
        "64":"images/internet64x64.png"
   },
   "background": {
        "service_worker": "background.js",
        "type": "module" 
   },
   "permissions": [
        "storage"
   ]
}
